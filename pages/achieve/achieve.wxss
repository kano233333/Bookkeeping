.achieve {
  min-width: 100vw;
  min-height: 100vh;
  padding-bottom: 6vw;
  background-color: #ededed;
}
.achieve_head {
  height: 40vw;
}
.achieve_head image {
  width: 100%;
  height: 100%;
}
.title {
  height: 5.33333333vw;
  box-shadow: 0 5px 8px -5px #787878;
  font-size: 2.4vw;
  line-height: 2;
  color: #333;
  padding-left: 5.6vw;
  letter-spacing: 2px;
}
.images {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  margin-bottom: 8vw;
}
.images image {
  opacity: 0;
  margin: 6.26666667vw 5.33333333vw 0;
  animation: 0.3s scale_show forwards;
}
.achieve_con:nth-of-type(1) .images image {
  width: 19.2vw;
  height: 17.33333333vw;
}
.achieve_con:nth-of-type(2) .images image {
  width: 19.2vw;
  height: 14.93333333vw;
}
.images image:nth-of-type(1) {
  animation-delay: 0.1s;
}
.images image:nth-of-type(2) {
  animation-delay: 0.2s;
}
.images image:nth-of-type(3) {
  animation-delay: 0.3s;
}
.images image:nth-of-type(4) {
  animation-delay: 0.4s;
}
.images image:nth-of-type(5) {
  animation-delay: 0.5s;
}
.images image:nth-of-type(6) {
  animation-delay: 0.6s;
}
.show_badge {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.2);
}
.show_badge .box {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 600rpx;
  height: 900rpx;
  text-align: center;
  box-shadow: 1px 1px 10px #b9b9b9;
}
