.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.jz {
  display: flex;
  justify-content: center;
  align-items: center;
}
.jz_y {
  display: flex;
  align-items: center;
}
.jz_y_between {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.nav_bg {
  height: 10.66666667vw;
  background-color: #6dabf1;
  color: #fff;
}
.bor_btm {
  border-bottom: 3px solid #6dabf1;
  color: #68a1df;
}
.all {
  min-height: 100vh;
  background-color: #ededed;
}
.select {
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  padding: 4vw 6.33333333vw 4vw 6.33333333vw;
}
.select > view {
  margin: 2.66666667vw 3vw;
}
.select > view text {
  display: block;
  text-align: center;
}
.text_s {
  font-size: 18px;
  background: linear-gradient(110deg, #bc994e 0%, #bc994e 33%, #d8c498 44%, #775f2d 55%, #bc994e 67%, #bc994e 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  animation: 5s text_animate infinite;
  background-size: 300%;
}
.chart_head {
  height: 10.66666667vw;
  border-bottom: 1px solid #ededed;
  font-size: 3.2vw;
  display: flex;
  align-items: center;
  font-size: 4vw;
}
.chart_head > picker {
  margin: 0 20px;
  padding: 0 2px;
  line-height: 3.2;
}
.chart_head .select_time {
  border: 1px solid #6dabf1;
  border-radius: 10px;
  overflow: hidden;
  background-color: #6dabf1;
  box-shadow: 0 0 2px #5180b4;
  margin-left: 2.66666667vw;
}
.chart_head .select_time > text {
  color: #fff;
  padding: 0.66666667vw 2vw;
  display: inline-block;
}
.chart_head .select_time .selected {
  background-color: #fff;
  color: #68a1df;
  box-shadow: 0 0 2px #5180b4 inset;
}
.add_wrap {
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.4);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 3;
}
.addX {
  width: 9vw;
  height: 9vw;
  padding: 3vw;
  border-radius: 50%;
  background: linear-gradient(to top right, #98c5f7, #4387d3);
  position: fixed;
  right: 5vw;
  bottom: 18px;
  animation: 0.5s add_cha forwards;
  box-shadow: 1px 1px 5px #959394;
  transition: 0.5s all;
  z-index: 3;
  font-size: 4vw;
}
.addX image {
  width: 100%;
  height: 100%;
}
.addX input {
  font-size: 4vw;
  padding: 3vw;
  text-align: center;
  border-bottom: 1px solid rgba(0, 0, 0, 0.2);
}
.add button {
  width: 100%;
  background: linear-gradient(to top right, #98c5f7, #4387d3);
  color: #fff;
  margin: 5vw 0;
}
@keyframes add_cha {
  100% {
    transform: rotate(360deg);
  }
}
@keyframes text_animate {
  0% {
    background-position-x: 0;
  }
  50% {
    background-position-x: -150%;
  }
  100% {
    background-position-x: 0;
  }
}
@keyframes scale_show {
  0% {
    transform: scale(0);
    opacity: 0;
  }
  100% {
    transform: scale(1);
    opacity: 1;
  }
}
@keyframes center_scale_show {
  0% {
    transform: scaleX(0);
    opacity: 0;
  }
  100% {
    opacity: 1;
    transform: scaleX(1);
  }
}
@keyframes cha_rotate {
  0% {
    transform: rotate(0);
  }
  10% {
    transform: rotate(360deg);
  }
  12% {
    transform: translateY(-5px);
  }
  13% {
    transform: translateY(0);
  }
  14% {
    transform: translateY(-2px);
  }
  15% {
    transform: translateY(0);
  }
  100% {
    transform: translateY(0);
  }
}
@keyframes upper_enter {
  0% {
    transform: translateY(-40px);
    opacity: 0;
  }
  100% {
    transform: translateY(0);
    opacity: 1;
  }
}
@keyframes left_enter {
  0% {
    transform: translateX(-80px);
    opacity: 0;
  }
  100% {
    transform: translateX(0);
    opacity: 1;
  }
}
